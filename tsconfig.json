{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "resolveJsonModule": true,
    "outDir": "dist",
    "types": ["vitest/globals"]
  },
  "exclude": ["node_modules"]
}
